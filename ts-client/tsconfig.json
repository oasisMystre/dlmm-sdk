{
  "compilerOptions": {
    "outDir": "dist",
    "typeRoots": ["./node_modules/@types"],
    "module": "commonjs",
    "target": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": false,
  }
}
